{
    "name": "tomorrow-sky/qywechat",
    "description": "企业微信SDK，基于pithyone/wechat包，拓展新增自定义api",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tsky\\Qywechat\\": "src/"
        }
    },
    "authors": [
        {
            "name": "tomorrow-sky"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.3",
        "ext-SimpleXML": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "doctrine/collections": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.23",
        "symfony/cache": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-foundation": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeWork\\Laravel\\WeWorkServiceProvider"
            ]
        }
    }
}
